	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
	extraClientlibs="[cq.author.accordion]"
	jcr:primaryType="nt:unstructured" jcr:title="Listing Properties"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/container">
		<items jcr:primaryType="nt:unstructured">
			<tabs jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/tabs"
				maximized="{Boolean}true">
				<items jcr:primaryType="nt:unstructured">
					<generic jcr:primaryType="nt:unstructured"
						jcr:title="Generic"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<column jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<id jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="ID" name="./id" />
									<pathCF jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
										fieldLabel="Path Content Fragment Offerta Commodity"
										name="./pathCF" required="true"
										rootPath="/content/dam/enigaseluce" />
									<furniture jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/select"
										fieldLabel="Fornitura" name="./furniture">
										<items jcr:primaryType="nt:unstructured">
											<option1 jcr:primaryType="nt:unstructured"
												text="Luce e Gas" value="dual" selected="true" />
											<option2 jcr:primaryType="nt:unstructured"
												text="Luce" value="luce" />
											<option3 jcr:primaryType="nt:unstructured" text="Gas"
												value="gas" />
										</items>
									</furniture>
									<contentType jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
										jcr:title="Tipologia Contenuto">
										<items jcr:primaryType="nt:unstructured">
											<partnership jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/foundation/form/checkbox"
												name="./partnership" text="Con Partnership" value="true"
												class="cq-dialog-checkbox-showhide"
												cq-dialog-checkbox-showhide-target=".partnership-showhide-target" />
											<partnershipFields
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/foundation/container"
												jcr:title="Gestione Partnership"
												class="hidden partnership-showhide-target"
												showhidetargetvalue="true">
												<items jcr:primaryType="nt:unstructured">
													<image granite:class="cmp-image__editor-file-upload"
														jcr:primaryType="nt:unstructured"
														sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
														autoStart="{Boolean}false" class="cq-droptarget"
														fileNameParameter="./image"
														fileReferenceParameter="./image"
														mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
														multiple="{Boolean}false" name="./imageName"
														title="Immagine" uploadUrl="${suffix.path}"
														fieldLabel="Immagine" useHTML5="{Boolean}true" />
													<name jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
														fieldLabel="Nome Offerta" name="./saleName" />
													<supply jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
														fieldLabel="Fornitura" name="./supply" />
												</items>
											</partnershipFields>
											<promo jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/foundation/form/checkbox"
												name="./promo" text="Con Promozione" value="true"
												class="cq-dialog-checkbox-showhide"
												cq-dialog-checkbox-showhide-target=".promo-showhide-target" />
											<promotionsFields
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/foundation/container"
												jcr:title="Gestione Promo"
												class="hidden promo-showhide-target"
												showhidetargetvalue="true">
