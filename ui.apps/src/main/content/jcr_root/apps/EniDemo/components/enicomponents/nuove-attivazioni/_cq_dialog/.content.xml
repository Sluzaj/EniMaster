<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
	jcr:primaryType="nt:unstructured" jcr:title="Properties"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/container">
		<items jcr:primaryType="nt:unstructured">
			<tabs jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/tabs"
				maximized="{Boolean}true">
				<items jcr:primaryType="nt:unstructured">
					<generic jcr:primaryType="nt:unstructured"
						jcr:title="Generic"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<column jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<id jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="ID" name="./id" />
									<furnitureType jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/select"
										fieldLabel="Fornitura" name="./furnitureType">
										<items jcr:primaryType="nt:unstructured">
											<option1 jcr:primaryType="nt:unstructured"
												text="Luce" value="luce" />
											<option2 jcr:primaryType="nt:unstructured" text="Gas"
												value="gas" />
										</items>
									</furnitureType>

									<pathCF1 jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
										fieldLabel="Path Content Fragment Offerta Commodity"
										name="./pathCF" rootPath="/content/dam/enigaseluce" />
									<pathJSON jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
										fieldLabel="Path Json testi di sistema" name="./pathJson"
										rootPath="/content/dam" />
									<title jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Titolo" name="./title" />
									<description jcr:primaryType="nt:unstructured"
										jcr:title="Descrizione" fieldLabel="Descrizione"
										sling:resourceType="cq/gui/components/authoring/dialog/richtext"
										name="./description" useFixedInlineToolbar="{Boolean}true">
										<rtePlugins jcr:primaryType="nt:unstructured"
											removeSingleParagraphContainer="true">
											<format jcr:primaryType="nt:unstructured"
												features="bold,italic" />
											<justify jcr:primaryType="nt:unstructured"
												features="*" />
											<links jcr:primaryType="nt:unstructured"
												features="modifylink,unlink" />
											<lists jcr:primaryType="nt:unstructured" features="*" />
											<paraformat jcr:primaryType="nt:unstructured"
												features="*">
												<formats jcr:primaryType="nt:unstructured">
													<default_p jcr:primaryType="nt:unstructured"
														description="Paragraph" tag="p" />
													<default_h1 jcr:primaryType="nt:unstructured"
														description="Heading 1" tag="h1" />
													<default_h2 jcr:primaryType="nt:unstructured"
														description="Heading 2" tag="h2" />
													<default_h3 jcr:primaryType="nt:unstructured"
														description="Heading 3" tag="h3" />
												</formats>
											</paraformat>
											<tracklinks jcr:primaryType="nt:unstructured"
												features="*" />
											<table jcr:primaryType="nt:unstructured" features="*" />
											<subsuperscript
												jcr:primaryType="nt:unstructured" features="*" />
											<misctools jcr:primaryType="nt:unstructured"
												features="*" />
											<experience-aem
												jcr:primaryType="nt:unstructured" features="*" />
										</rtePlugins>
										<uiSettings jcr:primaryType="nt:unstructured">
											<cui jcr:primaryType="nt:unstructured">
												<inline jcr:primaryType="nt:unstructured"
													toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,justify#justifyleft,justify#justifycenter,justify#justifyright,#lists,experience-aem#colorPicker,links#modifylink,links#unlink,#paraformat,table#createoredit,table#removetable,misctools#sourceedit]">
													<popovers jcr:primaryType="nt:unstructured">
														<justify jcr:primaryType="nt:unstructured"
															items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
															ref="justify" />
														<lists jcr:primaryType="nt:unstructured"
															items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
															ref="lists" />
														<misctools jcr:primaryType="nt:unstructured"
															items="[misctools#specialchars]" ref="misctools" />
														<paraformat jcr:primaryType="nt:unstructured"
															items="paraformat:getFormats:paraformat-pulldown"
															ref="paraformat" />
													</popovers>
												</inline>
												<dialogFullScreen
													jcr:primaryType="nt:unstructured"
													toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,justify#justifyleft,justify#justifycenter,justify#justifyright,#lists,experience-aem#colorPicker,links#modifylink,links#unlink,#paraformat,table#createoredit,table#removetable,misctools#sourceedit]">
													<popovers jcr:primaryType="nt:unstructured">
														<justify jcr:primaryType="nt:unstructured"
															items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
															ref="justify" />
														<lists jcr:primaryType="nt:unstructured"
															items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
															ref="lists" />
														<misctools jcr:primaryType="nt:unstructured"
															items="[misctools#specialchars]" ref="misctools" />
														<paraformat jcr:primaryType="nt:unstructured"
															items="paraformat:getFormats:paraformat-pulldown"
															ref="paraformat" />
													</popovers>
												</dialogFullScreen>
											</cui>
										</uiSettings>
									</description>
									<callme jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
										fieldLabel="Label CTA Richiamami" name="./callme"
										emptyText="Ti richiamiamo noi" value="Ti richiamiamo noi" 
										fieldDescription="Se viene compilato il campo label allora viene visualizzata la cta  “Ti richiamiamo noi”. Per eliminare la cta è sufficiente cancellare la label."/>
								</items>
							</column>
						</items>
					</generic>
					<faq jcr:primaryType="nt:unstructured"
						jcr:title="Riferimenti FAQ"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<column jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<gasFaq jcr:primaryType="nt:unstructured"
										jcr:title="Uscite Gas"
										sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
										granite:class="furnitureType-showhide-target">
										<items jcr:primaryType="nt:unstructured">
											<volturaGas
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured" fieldLabel="Voltura Gas"
												name="./volturaGas">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</volturaGas>
											<subentroGas
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured" fieldLabel="Subentro Gas"
												name="./subentroGas">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</subentroGas>
											<subentroSempliceGasNo
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Subentro Semplice Gas - Senza lavori su impianto"
												name="./subentroSempliceGasNo">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</subentroSempliceGasNo>
											<subentroSempliceGasNonSo
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Subentro Semplice Gas - Lavori su impianto non noti"
												name="./subentroSempliceGasNonSo">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</subentroSempliceGasNonSo>
											<allaccioPosaAttivazioneGasNoPdr
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Allaccio, posa e attivazione del contatore Gas - Senza PDR"
												name="./allaccioPosaAttivazioneGasNoPdr">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</allaccioPosaAttivazioneGasNoPdr>
											<allaccioPosaAttivazioneGasNo
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Allaccio, posa e attivazione del contatore Gas - Senza attacco"
												name="./allaccioPosaAttivazioneGasNo">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</allaccioPosaAttivazioneGasNo>
											<posaAttivazioneGas
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Posa e attivazione del contatore Gas - Con lavori su impianto"
												name="./posaAttivazioneGas">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</posaAttivazioneGas>
											<posaAttivazioneGasNo
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Posa e attivazione del contatore Gas - Senza lavori su impianto"
												name="./posaAttivazioneGasNo">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</posaAttivazioneGasNo>
											<posaAttivazioneGasNonSo
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Posa e attivazione del contatore Gas - Lavori su impianto non noti"
												name="./posaAttivazioneGasNonSo">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</posaAttivazioneGasNonSo>
										</items>
										<granite:data jcr:primaryType="nt:unstructured"
											showhidetargetvalue="gas" />
									</gasFaq>
									<luceFaq jcr:primaryType="nt:unstructured"
										jcr:title="Uscite Luce"
										sling:resourceType="granite/ui/components/coral/foundation/form/fieldset"
										granite:class="furnitureType-showhide-target">
										<items jcr:primaryType="nt:unstructured">
											<volturaLuce
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured" fieldLabel="Voltura Luce"
												name="./volturaLuce">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</volturaLuce>
											<swVolturaLuce
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Attivare un'offerta con voltura Luce"
												name="./swVolturaLuce">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</swVolturaLuce>
											<primaAttivazioneLuce
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Prima attivazione del contatore Luce"
												name="./primaAttivazioneLuce">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</primaAttivazioneLuce>
											<subentroLuce
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured" fieldLabel="Subentro Luce"
												name="./subentroLuce">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</subentroLuce>
											<posaAttivazioneLuce
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Posa e attivazione del contatore Luce"
												name="./posaAttivazioneLuce">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</posaAttivazioneLuce>
											<allaccioPosaAttivazioneLuce
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												jcr:primaryType="nt:unstructured"
												fieldLabel="Allaccio, posa e attivazione del contatore Luce"
												name="./allaccioPosaAttivazioneLuce">
												<datasource jcr:primaryType="nt:unstructured"
													sling:resourceType="enigaseluce/components/nuove-attivazioni/datasource-faq" />
											</allaccioPosaAttivazioneLuce>
										</items>
										<granite:data jcr:primaryType="nt:unstructured"
											showhidetargetvalue="luce" />
									</luceFaq>
								</items>
							</column>
						</items>
					</faq>
					<wa jcr:primaryType="nt:unstructured"
						jcr:title="Configurazione whatsapp"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<column jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container">
								<items jcr:primaryType="nt:unstructured">
									<whatsappluce   jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/foundation/form/checkbox"
												name="./whatsappluce" text="Variante Whatsapp luce" value="true" />
									
									<whatsappgas   jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/foundation/form/checkbox"
												name="./whatsappgas" text="Variante Whatsapp gas" value="true" />
									
								</items>
							</column>
						</items>
					</wa>
				</items>
			</tabs>
		</items>
	</content>
</jcr:root>