<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
	jcr:primaryType="nt:unstructured" jcr:title="Properties Cta"
	extraClientlibs="[cq.author.accordion]"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/container">
		<items jcr:primaryType="nt:unstructured">
			<tabs jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/tabs"
				maximized="{Boolean}true">
				<items jcr:primaryType="nt:unstructured">

					<proprietaComponente
						jcr:primaryType="nt:unstructured" jcr:title="ProprietÃ  Componente"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<columns jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container"
								margin="{Boolean}true">
								<items jcr:primaryType="nt:unstructured">
									<column jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container">
										<items jcr:primaryType="nt:unstructured">
											<ctaSelect jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												fieldLabel="Scelta Cta" name="./ctaSelect">
												<items jcr:primaryType="nt:unstructured">
													<option1 jcr:primaryType="nt:unstructured"
														text="Cta Primaria" value="rich-text-cta-primary" />
													<option2 jcr:primaryType="nt:unstructured"
														text="Cta Link" value="rich-text-cta-secondary" selected="true" />
												</items>
											</ctaSelect>
											<labelCTA jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="Label CTA" name="./labelCTA" />
											<urlCTA jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
												fieldDescription="Inserisci ulr per la CTA" required="true"
												fieldLabel="Path Link CTA" name="./urlCTA" />
											<targetCTA jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												fieldLabel="Target" name="./targetCTA">
												<items jcr:primaryType="nt:unstructured">
													<option1 jcr:primaryType="nt:unstructured"
														text="Self" value="_self" selected="true" />
													<option2 jcr:primaryType="nt:unstructured"
														text="Blank" value="_blank" />
												</items>
											</targetCTA>
											<altCTA jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="Alt Text CTA" name="./altCTA" />
										</items>
									</column>
								</items>
							</columns>
						</items>
					</proprietaComponente>


				</items>
			</tabs>
		</items>
	</content>
</jcr:root>
