<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
	jcr:primaryType="nt:unstructured" jcr:title="Properties Hero Commodity"
	extraClientlibs="[cq.author.accordion]"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/container">
		<items jcr:primaryType="nt:unstructured">
			<tabs jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/tabs"
				maximized="{Boolean}true">
				<items jcr:primaryType="nt:unstructured">

					<proprietaComponente
						jcr:primaryType="nt:unstructured" jcr:title="Proprietà Componente"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<columns jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container"
								margin="{Boolean}true">
								<items jcr:primaryType="nt:unstructured">
									<column jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container">
										<items jcr:primaryType="nt:unstructured">
											<id jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="ID Componente" name="./id" />
											<tipoHeroCommodity
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												granite:class="cq-dialog-dropdown-showhide"
												fieldLabel="Tipo Hero Commodity" required="true"
												name="./tipoHeroCommodity">
												<items jcr:primaryType="nt:unstructured">
													<option1 jcr:primaryType="nt:unstructured"
														text="Hero Commodity Basic" value="heroCommodity" />
													<option2 jcr:primaryType="nt:unstructured"
														text="Hero Commodity + KSPx2" value="heroCommodityKSPx2" />
													<option3 jcr:primaryType="nt:unstructured"
														text="Hero Commodity + KSP img"
														value="heroCommodityKSPimg" />
												</items>
												<granite:data jcr:primaryType="nt:unstructured"
													cq-dialog-dropdown-showhide-target=".heroCommodityTipo-showhide-target" />
											</tipoHeroCommodity>
										</items>
									</column>
								</items>

							</columns>
						</items>
					</proprietaComponente>



					<configurazioneComponente
						jcr:primaryType="nt:unstructured"
						jcr:title="Configurazione Componente"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<columns jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container"
								margin="{Boolean}true">
								<items jcr:primaryType="nt:unstructured">
									<column jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container">
										<items jcr:primaryType="nt:unstructured">
											<tipoContenuto
												jcr:primaryType="nt:unstructured"
												jcr:title="Tipologia di contenuto"
												sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
												<items jcr:primaryType="nt:unstructured">
													<compilazioneComponente
														jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/select"
														granite:class="cq-dialog-dropdown-showhide"
														fieldLabel="Modalità Compilazione Componente"
														required="true" name="./compilazioneComponente">
														<items jcr:primaryType="nt:unstructured">
															<option1 jcr:primaryType="nt:unstructured"
																text="Content Fragment" value="contentFragment" />
															<option2 jcr:primaryType="nt:unstructured"
																text="Editoriale" value="editoriale" />
														</items>
														<granite:data
															jcr:primaryType="nt:unstructured"
															cq-dialog-dropdown-showhide-target=".compilazioneComp-showhide-target" />
													</compilazioneComponente>

													<fragment
														granite:class="compilazioneComp-showhide-target"
														jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/container">
														<items jcr:primaryType="nt:unstructured">
															<pathFragment
																jcr:primaryType="nt:unstructured"
																sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
																fieldLabel="Path Content Fragment Offerta"
																name="./pathFragment"
																rootPath="/content/dam/enigaseluce" />
														</items>
														<granite:data
															jcr:primaryType="nt:unstructured"
															showhidetargetvalue="contentFragment" />
													</fragment>
													<pathPromo1 jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
														fieldLabel="Path Content Fragment Promozione 1"
														name="./pathFragment2" rootPath="/content/dam/enigaseluce" />
													<pathPromo2 jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
														fieldLabel="Path Content Fragment Promozione 2"
														name="./pathFragment3" rootPath="/content/dam/enigaseluce" />

												</items>
											</tipoContenuto>

											<heroCommodityBasic
												jcr:primaryType="nt:unstructured"
												jcr:title="Configurazione campi base"
												sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
												<items jcr:primaryType="nt:unstructured">
													<titoloH1 jcr:primaryType="nt:unstructured"
														jcr:title="Titolo" fieldLabel="Titolo"
														sling:resourceType="cq/gui/components/authoring/dialog/richtext"
														name="./titoloH1" required="true"
														fieldDescription="Configurazione obbligatoria sia in versione editoriale che con content fragment"
														useFixedInlineToolbar="{Boolean}true"
														removeSingleParagraphContainer="{Boolean}true">
														<rtePlugins jcr:primaryType="nt:unstructured"
															removeSingleParagraphContainer="true">
															<format jcr:primaryType="nt:unstructured"
																features="bold,italic" />
															<justify jcr:primaryType="nt:unstructured"
																features="*" />
															<links jcr:primaryType="nt:unstructured"
																features="modifylink,unlink" />
															<lists jcr:primaryType="nt:unstructured"
																features="*" />
															<paraformat jcr:primaryType="nt:unstructured"
																features="*">
																<formats jcr:primaryType="nt:unstructured">
																	<default_p jcr:primaryType="nt:unstructured"
																		description="Paragraph" tag="p" />
																	<default_h1
																		jcr:primaryType="nt:unstructured"
																		description="Heading 1" tag="h1" />
																	<default_h2
																		jcr:primaryType="nt:unstructured"
																		description="Heading 2" tag="h2" />
																	<default_h3
																		jcr:primaryType="nt:unstructured"
																		description="Heading 3" tag="h3" />
																</formats>
															</paraformat>
															<tracklinks jcr:primaryType="nt:unstructured"
																features="*" />
															<table jcr:primaryType="nt:unstructured"
																features="*" />
															<subsuperscript
																jcr:primaryType="nt:unstructured" features="*" />
															<misctools jcr:primaryType="nt:unstructured"
																features="*" />
															<experience-aem
																jcr:primaryType="nt:unstructured" features="*" />
														</rtePlugins>
														<uiSettings jcr:primaryType="nt:unstructured">
															<cui jcr:primaryType="nt:unstructured">
																<inline jcr:primaryType="nt:unstructured"
																	toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,justify#justifyleft,justify#justifycenter,justify#justifyright,#lists,experience-aem#colorPicker,links#modifylink,links#unlink,#paraformat,table#createoredit,table#removetable,misctools#sourceedit]">
																	<popovers jcr:primaryType="nt:unstructured">
																		<justify jcr:primaryType="nt:unstructured"
																			items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
																			ref="justify" />
																		<lists jcr:primaryType="nt:unstructured"
																			items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
																			ref="lists" />
																		<misctools jcr:primaryType="nt:unstructured"
																			items="[misctools#specialchars]" ref="misctools" />
																		<paraformat
																			jcr:primaryType="nt:unstructured"
																			items="paraformat:getFormats:paraformat-pulldown"
																			ref="paraformat" />
																	</popovers>
																</inline>
																<dialogFullScreen
																	jcr:primaryType="nt:unstructured"
																	toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,justify#justifyleft,justify#justifycenter,justify#justifyright,#lists,experience-aem#colorPicker,links#modifylink,links#unlink,#paraformat,table#createoredit,table#removetable,misctools#sourceedit]">
																	<popovers jcr:primaryType="nt:unstructured">
																		<justify jcr:primaryType="nt:unstructured"
																			items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
																			ref="justify" />
																		<lists jcr:primaryType="nt:unstructured"
																			items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
																			ref="lists" />
																		<misctools jcr:primaryType="nt:unstructured"
																			items="[misctools#specialchars]" ref="misctools" />
																		<paraformat
																			jcr:primaryType="nt:unstructured"
																			items="paraformat:getFormats:paraformat-pulldown"
																			ref="paraformat" />
																	</popovers>
																</dialogFullScreen>
															</cui>
														</uiSettings>
													</titoloH1>

													<!-- Logo 1 -->
													<logo1 granite:class="cmp-image__editor-file-upload"
														jcr:primaryType="nt:unstructured"
														sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
														autoStart="{Boolean}false" class="cq-droptarget"
														fileNameParameter="./logo1Name"
														fileReferenceParameter="./logo1Reference"
														mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
														multiple="{Boolean}false" name="./logo1" title="Logo 1"
														fieldLabel="Logo 1" uploadUrl="${suffix.path}"
														useHTML5="{Boolean}true" />
													<urlLogo1 jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
														fieldLabel="Path URL Logo 1" name="./urlLogo1" />
													<targetLogo1 jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/select"
														fieldLabel="Target Logo 1" name="./targetLogo1">
														<items jcr:primaryType="nt:unstructured">
															<option1 jcr:primaryType="nt:unstructured"
																selected="true" text="Self" value="_self" />
															<option2 jcr:primaryType="nt:unstructured"
																text="Blank" value="_blank" />
														</items>
													</targetLogo1>

													<!-- Separatore Logo -->
													<coloreSeparatore
														jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/select"
														fieldLabel="Colore Separatore Logo"
														name="./coloreSeparatore">
														<items jcr:primaryType="nt:unstructured">
															<option1 jcr:primaryType="nt:unstructured"
																text="Gray" value="#333333" />
															<option2 jcr:primaryType="nt:unstructured"
																text="White" value="#ffffff" />
														</items>
													</coloreSeparatore>


													<!-- Logo 2 -->
													<logo2 granite:class="cmp-image__editor-file-upload"
														jcr:primaryType="nt:unstructured"
														sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
														autoStart="{Boolean}false" class="cq-droptarget"
														fileNameParameter="./logo2Name"
														fileReferenceParameter="./logo2Reference"
														mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
														multiple="{Boolean}false" name="./logo2" title="Logo 2"
														fieldLabel="Logo 2" uploadUrl="${suffix.path}"
														useHTML5="{Boolean}true" />
													<urlLogo2 jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
														fieldLabel="Path URL Logo 2" name="./urlLogo2" />
													<targetLogo2 jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/select"
														fieldLabel="Target Logo 2" name="./targetLogo2">
														<items jcr:primaryType="nt:unstructured">
															<option1 jcr:primaryType="nt:unstructured"
																selected="true" text="Self" value="_self" />
															<option2 jcr:primaryType="nt:unstructured"
																text="Blank" value="_blank" />
														</items>
													</targetLogo2>
													
													<coloreVelina
														jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/select"
														fieldLabel="Colore Velina"
														granite:class="cq-dialog-dropdown-showhide"
														name="./coloreVelina">
														<items jcr:primaryType="nt:unstructured">
															<option1 jcr:primaryType="nt:unstructured"
																text="Grigio" value="filtro-background-img-hero-nero" />
															<option2 jcr:primaryType="nt:unstructured"
																text="Bianco" value="filtro-background-img-hero-bianco" />
															<option3 jcr:primaryType="nt:unstructured"
																text="Colore Esadecimale" value="filtro-background-img-hero-colorato" />
															<option4 jcr:primaryType="nt:unstructured"
																text="Nessuna Velina" value="" selected="true" />
														</items>
														<granite:data
															jcr:primaryType="nt:unstructured"
															cq-dialog-dropdown-showhide-target=".coloreVelinaEsa-showhide-target" />
													</coloreVelina>
														
													<coloreEsadecimale
														granite:class="coloreVelinaEsa-showhide-target"
														jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/container">
														<items jcr:primaryType="nt:unstructured">
															<coloreVelinaEsadecimale1
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
																		fieldLabel="Colore Velina"
																		name="./colore1" />
														</items>
														<granite:data
															jcr:primaryType="nt:unstructured"
															showhidetargetvalue="filtro-background-img-hero-colorato" />
													</coloreEsadecimale>
													
													
												</items>
											</heroCommodityBasic>

											<configurazioneEditoriale
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/container"
												granite:class="compilazioneComp-showhide-target"
												margin="{Boolean}true">
												<items jcr:primaryType="nt:unstructured">
													<heroCommodityBasicEditoriale
														jcr:primaryType="nt:unstructured"
														sling:resourceType="granite/ui/components/coral/foundation/form/fieldset">
														<items jcr:primaryType="nt:unstructured">

															<tipoBackground
																jcr:primaryType="nt:unstructured"
																sling:resourceType="granite/ui/components/coral/foundation/form/select"
																granite:class="cq-dialog-dropdown-showhide"
																fieldLabel="Background Color" required="true"
																name="./tipoBackground">
																<items jcr:primaryType="nt:unstructured">
																	<option1 jcr:primaryType="nt:unstructured"
																		text="Immagine" value="immagineBack" />
																	<option2 jcr:primaryType="nt:unstructured"
																		text="Colore esadecimale" value="coloreBack" />
																</items>
																<granite:data
																	jcr:primaryType="nt:unstructured"
																	cq-dialog-dropdown-showhide-target=".heroCommodityBG-showhide-target" />
															</tipoBackground>

															<coloreBack
																granite:class="heroCommodityBG-showhide-target"
																jcr:primaryType="nt:unstructured"
																sling:resourceType="granite/ui/components/coral/foundation/container">
																<items jcr:primaryType="nt:unstructured">
																	<coloreBackground
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="granite/ui/components/coral/foundation/form/colorfield"
																		fieldLabel="Colore Background (esadecimale)"
																		value="#ffffff" name="./coloreBackground" />
																</items>
																<granite:data
																	jcr:primaryType="nt:unstructured"
																	showhidetargetvalue="coloreBack" />
															</coloreBack>

															<immagineBack
																granite:class="heroCommodityBG-showhide-target"
																jcr:primaryType="nt:unstructured"
																sling:resourceType="granite/ui/components/coral/foundation/container">
																<items jcr:primaryType="nt:unstructured">
																	<assetImmagine
																		granite:class="cmp-image__editor-file-upload"
																		jcr:primaryType="nt:unstructured"
																		sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
																		autoStart="{Boolean}false" class="cq-droptarget"
																		fileNameParameter="./immagineBackName"
																		fileReferenceParameter="./immagineBackReference"
																		mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
																		multiple="{Boolean}false" name="./immagineBack"
																		title="Asset Immagine" fieldLabel="Asset Immagine"
																		uploadUrl="${suffix.path}" useHTML5="{Boolean}true" />
																</items>
																<granite:data
																	jcr:primaryType="nt:unstructured"
																	showhidetargetvalue="immagineBack" />
															</immagineBack>

