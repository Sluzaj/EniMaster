<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
	xmlns:cq="http://www.day.com/jcr/cq/1.0"
	xmlns:jcr="http://www.jcp.org/jcr/1.0"
	xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
	jcr:primaryType="nt:unstructured" jcr:title="Properties Tab Listing"
	sling:resourceType="cq/gui/components/authoring/dialog">
	<content jcr:primaryType="nt:unstructured"
		sling:resourceType="granite/ui/components/coral/foundation/container">
		<items jcr:primaryType="nt:unstructured">
			<tabs jcr:primaryType="nt:unstructured"
				sling:resourceType="granite/ui/components/coral/foundation/tabs"
				maximized="{Boolean}true">
				<items jcr:primaryType="nt:unstructured">
					<id jcr:primaryType="nt:unstructured" jcr:title="ID"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<columns jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container"
								margin="{Boolean}true">
								<items jcr:primaryType="nt:unstructured">
									<column jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container">
										<items jcr:primaryType="nt:unstructured">
											<id jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="ID" name="./id" />
										</items>
									</column>
								</items>
							</columns>
						</items>
					</id>
					<titoloH4 jcr:primaryType="nt:unstructured"
						jcr:title="Titolo H4" fieldLabel="Titolo H4"
						sling:resourceType="cq/gui/components/authoring/dialog/richtext"
						name="./titoloH4" required="true"
						useFixedInlineToolbar="{Boolean}true"
						removeSingleParagraphContainer="{Boolean}true">
						<rtePlugins jcr:primaryType="nt:unstructured"
							removeSingleParagraphContainer="true">
							<format jcr:primaryType="nt:unstructured"
								features="bold,italic" />
							<justify jcr:primaryType="nt:unstructured" features="*" />
							<links jcr:primaryType="nt:unstructured"
								features="modifylink,unlink" />
							<lists jcr:primaryType="nt:unstructured" features="*" />
							<paraformat jcr:primaryType="nt:unstructured"
								features="*">
								<formats jcr:primaryType="nt:unstructured">
									<default_p jcr:primaryType="nt:unstructured"
										description="Paragraph" tag="p" />
									<default_h1 jcr:primaryType="nt:unstructured"
										description="Heading 1" tag="h1" />
									<default_h2 jcr:primaryType="nt:unstructured"
										description="Heading 2" tag="h2" />
									<default_h3 jcr:primaryType="nt:unstructured"
										description="Heading 3" tag="h3" />
								</formats>
							</paraformat>
							<tracklinks jcr:primaryType="nt:unstructured"
								features="*" />
							<table jcr:primaryType="nt:unstructured" features="*" />
							<subsuperscript jcr:primaryType="nt:unstructured"
								features="*" />
							<misctools jcr:primaryType="nt:unstructured"
								features="*" />
							<experience-aem jcr:primaryType="nt:unstructured"
								features="*" />
						</rtePlugins>
						<uiSettings jcr:primaryType="nt:unstructured">
							<cui jcr:primaryType="nt:unstructured">
								<inline jcr:primaryType="nt:unstructured"
									toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,justify#justifyleft,justify#justifycenter,justify#justifyright,#lists,experience-aem#colorPicker,links#modifylink,links#unlink,#paraformat,table#createoredit,table#removetable,misctools#sourceedit]">
									<popovers jcr:primaryType="nt:unstructured">
										<justify jcr:primaryType="nt:unstructured"
											items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
											ref="justify" />
										<lists jcr:primaryType="nt:unstructured"
											items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
											ref="lists" />
										<misctools jcr:primaryType="nt:unstructured"
											items="[misctools#specialchars]" ref="misctools" />
										<paraformat jcr:primaryType="nt:unstructured"
											items="paraformat:getFormats:paraformat-pulldown"
											ref="paraformat" />
									</popovers>
								</inline>
								<dialogFullScreen
									jcr:primaryType="nt:unstructured"
									toolbar="[format#bold,format#italic,format#underline,subsuperscript#subscript,subsuperscript#superscript,justify#justifyleft,justify#justifycenter,justify#justifyright,#lists,experience-aem#colorPicker,links#modifylink,links#unlink,#paraformat,table#createoredit,table#removetable,misctools#sourceedit]">
									<popovers jcr:primaryType="nt:unstructured">
										<justify jcr:primaryType="nt:unstructured"
											items="[justify#justifyleft,justify#justifycenter,justify#justifyright]"
											ref="justify" />
										<lists jcr:primaryType="nt:unstructured"
											items="[lists#unordered,lists#ordered,lists#outdent,lists#indent]"
											ref="lists" />
										<misctools jcr:primaryType="nt:unstructured"
											items="[misctools#specialchars]" ref="misctools" />
										<paraformat jcr:primaryType="nt:unstructured"
											items="paraformat:getFormats:paraformat-pulldown"
											ref="paraformat" />
									</popovers>
								</dialogFullScreen>
							</cui>
						</uiSettings>
					</titoloH4>

					<primoCTA jcr:primaryType="nt:unstructured"
						jcr:title="Primo CTA"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<columns jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container"
								margin="{Boolean}true">
								<items jcr:primaryType="nt:unstructured">
									<column jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container">
										<items jcr:primaryType="nt:unstructured">
											<labelPrimoCTA
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="Label Primo CTA" required="true"
												name="./labelPrimoCTA" />
											<urlPrimoCTA jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
												fieldDescription="Inserisci il link per il primo CTA"
												fieldLabel="Path Link Primo CTA" name="./urlPrimoCTA"
												required="true" />
											<targetPrimoCTA
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												fieldLabel="Target" name="./targetPrimoCTA">
												<items jcr:primaryType="nt:unstructured">
													<option1 jcr:primaryType="nt:unstructured"
														text="Self" value="_self" selected="true" />
													<option2 jcr:primaryType="nt:unstructured"
														text="Blank" value="_blank" />
												</items>
											</targetPrimoCTA>
											<altPrimoCTA jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="Alt Text" name="./altPrimoCTA" />
										</items>
									</column>
								</items>
							</columns>
						</items>
					</primoCTA>

					<secondoCTA jcr:primaryType="nt:unstructured"
						jcr:title="Secondo CTA"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<columns jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container"
								margin="{Boolean}true">
								<items jcr:primaryType="nt:unstructured">
									<column jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container">
										<items jcr:primaryType="nt:unstructured">
											<labelSecondoCTA
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="Label Secondo CTA" required="true"
												name="./labelSecondoCTA" />
											<urlSecondoCTA
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
												fieldDescription="Inserisci il link per il secondo CTA"
												fieldLabel="Path Link Secondo CTA" name="./urlSecondoCTA"
												required="true" />
											<targetSecondoCTA
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												fieldLabel="Target" name="./targetSecondoCTA">
												<items jcr:primaryType="nt:unstructured">
													<option1 jcr:primaryType="nt:unstructured"
														text="Self" value="_self" selected="true" />
													<option2 jcr:primaryType="nt:unstructured"
														text="Blank" value="_blank" />
												</items>
											</targetSecondoCTA>
											<altSecondoCTA
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="Alt Text" name="./altSecondoCTA" />
										</items>
									</column>
								</items>
							</columns>
						</items>
					</secondoCTA>

					<terzoCTA jcr:primaryType="nt:unstructured"
						jcr:title="Terzo CTA"
						sling:resourceType="granite/ui/components/coral/foundation/container"
						margin="{Boolean}true">
						<items jcr:primaryType="nt:unstructured">
							<columns jcr:primaryType="nt:unstructured"
								sling:resourceType="granite/ui/components/coral/foundation/container"
								margin="{Boolean}true">
								<items jcr:primaryType="nt:unstructured">
									<column jcr:primaryType="nt:unstructured"
										sling:resourceType="granite/ui/components/coral/foundation/container">
										<items jcr:primaryType="nt:unstructured">
											<labelTerzoCTA
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="Label terzo CTA" required="true"
												name="./labelTerzoCTA" />
											<urlTerzoCTA jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
												fieldDescription="Inserisci il link per il terzo CTA"
												fieldLabel="Path Link Terzo CTA" name="./urlTerzoCTA"
												required="true" />
											<targetTerzoCTA
												jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/select"
												fieldLabel="Target" name="./targetTerzoCTA">
												<items jcr:primaryType="nt:unstructured">
													<option1 jcr:primaryType="nt:unstructured"
														text="Self" value="_self" selected="true" />
													<option2 jcr:primaryType="nt:unstructured"
														text="Blank" value="_blank" />
												</items>
											</targetTerzoCTA>
											<altTerzoCTA jcr:primaryType="nt:unstructured"
												sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
												fieldLabel="Alt Text" name="./altTerzoCTA" />
										</items>
									</column>
								</items>
							</columns>
						</items>
					</terzoCTA>
				</items>
			</tabs>
		</items>
	</content>
</jcr:root>
